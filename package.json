{
  "name": "gitcom",
  "displayName": "GitCom",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "gitcom",
    "comment",
    "comments",
    "delete",
    "remove"
  ],
  "activationEvents": [
    "onCommand:gitcom.removeAllComments",
    "onCommand:gitcom.saveComments",
    "onCommand:gitcom.createFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Remove Comments configuration",
      "properties": {
        "gitcom.multilineComments": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "gitcom.removeAllComments",
        "title": "GitCom: Remove all comments"
      },
      {
        "command": "gitcom.saveComments",
        "title": "GitCom: Save Comments"
      },
      {
        "command": "gitcom.createFile",
        "title": "GitCom: Create file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@types/mkdirp": "^1.0.1",
    "watch": "^1.0.2"
  }
}
